<h1>Events</h1>

<h2>Past Events:</h2>
<ul>
<% @past_events.each do |event| %>
  <li>
    <%= link_to event.name, event_path(event.id)  %>
  </li>

<% end %>
</ul>

<h2>Upcoming Events:</h2>
<ul>
<% @upcoming_events.each do |event| %>
  <li>
    <%= link_to event.name, event_path(event.id)  %>
  </li>

<% end %>
</ul>
